<?xml version='1.0'?>
<package xmlns='http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd'>
        <metadata>
                <id>FacebookWebMvc</id>
                <version><%= version_full %></version>
                <title/>
                <authors>Jim Zimmerman, Nathan Totten, Prabir Shrestha</authors>
                <description>The Facebook C# SDK MVC component.</description>
                <language>en-US</language>
                <licenseUrl>http://facebooksdk.codeplex.com/license</licenseUrl>
                <projectUrl>http://facebooksdk.codeplex.com</projectUrl>
                <iconUrl>http://bit.ly/facebooksdkicon</iconUrl>
                <requireLicenseAcceptance>false</requireLicenseAcceptance>
                <tags>Facebook</tags>
                <dependencies>
                        <dependency id='FacebookWeb' version='<%= version_full %>'/>
                </dependencies>
        </metadata>
        <files>
        	<file src="..\..\Source\Facebook.Web.Mvc\**\*.cs" target = "src" />
        	<file src="..\..\Source\Facebook.Web.Mvc\**\*.resx" target = "src" />
        	<file src="..\..\Bin\Release\net40\Facebook.Web.Mvc.dll" target = "lib\net40" />
        	<file src="..\..\Bin\Release\net40\Facebook.Web.Mvc.xml" target = "lib\net40" />
        	<file src="..\..\Bin\Release\net40\Facebook.Web.Mvc.pdb" target = "lib\net40" />
        	<file src="..\..\Bin\Release\net35\Facebook.Web.Mvc.dll" target = "lib\net35" />
        	<file src="..\..\Bin\Release\net35\Facebook.Web.Mvc.xml" target = "lib\net35" />
        	<file src="..\..\Bin\Release\net35\Facebook.Web.Mvc.pdb" target = "lib\net35" />
        </files>
</package>
