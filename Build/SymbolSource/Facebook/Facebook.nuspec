<?xml version='1.0'?>
<package xmlns='http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd'>
    <metadata>
        <id>Facebook</id>
        <version>$version$</version>
        <title/>
        <authors>Outercurve Foundation</authors>
        <owners>Sanjeev Dwivedi, Prabir Shrestha</owners>
        <description>The Facebook SDK for .NET helps developers build web, desktop, phone and windows store applications that integrate with Facebook.</description>
        <language>en-US</language>
        <licenseUrl>https://raw.github.com/facebook-csharp-sdk/facebook-csharp-sdk/master/LICENSE.txt</licenseUrl>
        <projectUrl>https://github.com/facebook-csharp-sdk/facebook-csharp-sdk</projectUrl>
        <iconUrl>http://csharpsdk.org/assets/img/logo75x75.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>Facebook fb</tags>
    </metadata>
    <files>
        
        <!-- Sources, licenses and README -->
        <file src="..\..\..\LICENSE.txt" target = "LICENSE.txt" />
        <file src="..\..\..\Build\readme.txt" target = "readme.txt" />
        <file src="..\..\..\Source\Facebook\**\*.cs" target = "src\Facebook" />
        <file src="..\..\..\Source\Facebook\**\*.resx" target = "src\Facebook" />
        
        <!-- .NET 4.5 architecture -->
        <file src="..\..\..\Bin\Release\net45\*.dll" target = "lib\net45" />
        <file src="..\..\..\Bin\Release\net45\*.xml" target = "lib\net45" />
        <file src="..\..\..\Bin\Release\net45\*.pdb" target = "lib\net45" />
        
        <!-- .net 4.0 architecture -->
        <file src="..\..\..\Bin\Release\net40-client\*.dll" target = "lib\net40-client" />
        <file src="..\..\..\Bin\Release\net40-client\*.xml" target = "lib\net40-client" />
        <file src="..\..\..\Bin\Release\net40-client\*.pdb" target = "lib\net40-client" />
        
        <!-- .net 3.5 architecture -->
        <file src="..\..\..\Bin\Release\net35-client\*.dll" target = "lib\net35-client" />
        <file src="..\..\..\Bin\Release\net35-client\*.xml" target = "lib\net35-client" />
        <file src="..\..\..\Bin\Release\net35-client\*.pdb" target = "lib\net35-client" />
        
        <!-- Silverlight architecture -->
        <file src="..\..\..\Bin\Release\sl5\*.dll" target = "lib\sl5" />
        <file src="..\..\..\Bin\Release\sl5\*.xml" target = "lib\sl5" />
        <file src="..\..\..\Bin\Release\sl5\*.pdb" target = "lib\sl5" />
        
        <!-- Windows Phone 7 architecture -->
        <file src="..\..\..\Bin\Release\wp71\*.dll" target = "lib\wp71" />
        <file src="..\..\..\Bin\Release\wp71\*.xml" target = "lib\wp71" />
        <file src="..\..\..\Bin\Release\wp71\*.pdb" target = "lib\wp71" />
        
        <!-- Windows Phone 8.0 architecture -->
        <file src="..\..\..\Bin\Release\wp8\*.dll" target = "lib\wp8" />
        <file src="..\..\..\Bin\Release\wp8\*.xml" target = "lib\wp8" />
        <file src="..\..\..\Bin\Release\wp8\*.pdb" target = "lib\wp8" />
    
        <!-- win8 architecture -->
        <file src="..\..\..\Bin\Release\win8\*.dll" target = "lib\win8" />
        <file src="..\..\..\Bin\Release\win8\*.xml" target = "lib\win8" />
        <file src="..\..\..\Bin\Release\win8\*.pdb" target = "lib\win8" />

    
        <!-- Windows Phone silverlight 8.1 architecture -->
        <file src="..\..\..\Bin\Release\wp81sl\*.dll" target = "lib\wp81" />
        <file src="..\..\..\Bin\Release\wp81sl\*.xml" target = "lib\wp81" />
        <file src="..\..\..\Bin\Release\wp81sl\*.pdb" target = "lib\wp81" />
        
        <!-- portable Windows and phone 8.1 architecture -->
        <file src="..\..\..\Bin\Release\win81universal\*.dll" target = "lib\portable-win81+wpa81" />
        <file src="..\..\..\Bin\Release\win81universal\*.pdb" target = "lib\portable-win81+wpa81" />
        <file src="..\..\..\Bin\Release\win81universal\*.xml" target = "lib\portable-win81+wpa81" />
		
        <!-- .NET Core 1.1 architecture -->
        <file src="..\..\..\Bin\Release\netcoreapp1.1\*.dll" target = "lib\netcoreapp1.1" />
        <file src="..\..\..\Bin\Release\netcoreapp1.1\*.pdb" target = "lib\netcoreapp1.1" />
        <file src="..\..\..\Bin\Release\netcoreapp1.1\*.xml" target = "lib\netcoreapp1.1" />
		
    </files>
</package>
