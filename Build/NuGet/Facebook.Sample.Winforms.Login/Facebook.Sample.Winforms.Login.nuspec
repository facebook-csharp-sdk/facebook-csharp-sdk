<?xml version='1.0'?>
<package xmlns='http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd'>
        <metadata>
                <id>Facebook.Sample.Winforms.Login</id>
                <version><%= version_full %></version>
                <title/>
                <authors>Jim Zimmerman, Nathan Totten, Prabir Shrestha</authors>
                <description>This package contains samples that demonstrate the use of the Facebook Login in WinForms in C#.</description>
                <language>en-US</language>
                <licenseUrl>http://facebooksdk.codeplex.com/license</licenseUrl>
                <projectUrl>http://facebooksdk.codeplex.com</projectUrl>
                <iconUrl>http://bit.ly/facebooksdkicon</iconUrl>
                <requireLicenseAcceptance>false</requireLicenseAcceptance>
                <tags>Facebook</tags>
                <dependencies>
                        <dependency id='Facebook' version='<%= version_full %>'/>
                </dependencies>
        </metadata>
        <files>
        	<file src="..\..\Build\NuGet\Facebook.Sample.Winforms.Login\content\**\*.*" target = "content" />
        	<file src="..\..\Build\NuGet\Facebook.Sample.Winforms.Login\tools\**\*" target = "tools" />
        </files>
</package>
